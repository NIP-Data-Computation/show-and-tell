        "df_temp_4 = df[['Arrested']].dropna()\n",
        "df_temp_4['Hour'] = df_temp_4.index.hour\n",
        "df_temp_4['Year'] = df_temp_4.index.year\n",
        "hourly_arrest_rate = df_temp_4.groupby('Hour').Arrested.mean()\n",
        "yearly_arrest_rate = df_temp_4.groupby('Year').Arrested.mean()\n",
        "hourly_arrest_rate.plot()\n",
        "plt.xlabel('Hour')\n",
        "plt.ylabel('Arrest Rate')\n",
        "plt.title('Arrest Rate by Time of Day')\n",
        "plt.show()\n",
